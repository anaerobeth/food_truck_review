<h1>All the food trucks</h1>
<%= paginate @food_trucks %>
<br />
<ul>
  <% @food_trucks.each do |food_truck| %>
    <li><%= link_to food_truck.name, food_truck_reviews_path(food_truck)%></li>
  <% end %>
</ul>

<% if current_user %>
  <%= link_to 'Add food truck', new_user_food_truck_path(current_user) %>
<% end %>
